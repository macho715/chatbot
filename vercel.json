{
  "version": 2,
  "name": "mosb-gate-agent",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "silent": false
  }
} 
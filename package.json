{
  "name": "mosb-gate-agent",
  "version": "2.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:unit": "jest __tests__/simple.test.ts __tests__/MatchingService.test.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "jest --coverage --watchAll=false",
    "test:mosb": "jest --testPathPattern=MOSB",
    "dev:mosb": "next dev --port 3001",
    "build:analyze": "ANALYZE=true next build",
    "type-check:mosb": "tsc --noEmit --project tsconfig.json",
    "lint": "echo 'Linting not configured'",
    "type-check": "echo 'Type checking not configured'",
    "security-audit": "npm audit --audit-level=moderate",
    "dependencies-check": "npm outdated || echo 'Dependencies check completed'"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "autoprefixer": "^10.4.0",
    "html5-qrcode": "^2.3.8",
    "next": "^12.3.4",
    "postcss": "^8.4.0",
    "qrcode.react": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "24.2.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
